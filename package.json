    "@angular/core": "5.2.x",
    "@angular/platform-browser": "5.2.x",
    "@angular/platform-browser-dynamic": "5.2.x",
    "@types/node": "6.x.x",
    "camelcase": "4.x.x",
    "concurrently": "3.5.x",
    "core-js": "2.5.x",
    "glob": "7.x.x",
    "lite-server": "2.2.x",
    "rimraf": "2.6.x",
    "rollup": "0.56.x",
    "rollup-plugin-sourcemaps": "0.4.x",
    "rollup-plugin-uglify": "3.0.x",
